{
  "expo": {
    "name": "ChiroClick",
    "slug": "chiroclick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "chiroclick",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#007AFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "no.chiroclick.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Brukes for profilbilde",
        "NSPhotoLibraryUsageDescription": "Brukes for profilbilde"
      }
    },
    "android": {
      "package": "no.chiroclick.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#007AFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#007AFF",
          "sounds": []
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "https://api.chiroclick.no/api/v1",
      "googleClientId": "",
      "googleIosClientId": "",
      "googleAndroidClientId": "",
      "googleWebClientId": "",
      "eas": {
        "projectId": ""
      }
    },
    "owner": "chiroclick",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/chiroclickcrm"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
